&#9;&#9;&#9;&#9;if v~=nil then
&#9;&#9;&#9;&#9;&#9;v:remove()
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;&#9;ct:clone().Parent=h
&#9;&#9;&#9;h:TakeDamage(damage)
