				<string name="Source">local Tool = script.Parent;

enabled = true


Calibrate = Instance.new(&quot;Sound&quot;)
Calibrate.SoundId = &quot;http://www.roblox.com/asset/?id=11956590&quot;
Calibrate.Parent = Tool.Handle



function onActivated()
	if not enabled  then
		return
	end

	enabled = false

	

	local sound = Instance.new(&quot;Sound&quot;)
		sound.SoundId = &quot;rbxasset://sounds\\Rocket shot.wav&quot;
		sound.Parent = script.Parent
		sound.PlayOnRemove = true
		sound.Volume = 1
		

	Tool.GripForward = Vector3.new(1,0,0)
	Tool.GripPos = Vector3.new(.5,-1.5,-1.56)
	Tool.GripRight = Vector3.new(0,-.707,-.707)
	Tool.GripUp = Vector3.new(0,-.707,.707)

	Calibrate:Play()
	wait(2)


	

	local e = Instance.new(&quot;Explosion&quot;)
	e.BlastRadius = 2
	e.BlastPressure = 1000000
	e.Position = Tool.Handle.Position
	e.Parent = game.Workspace

	wait(.1)
	Tool.Parent = game.Workspace --drop teddy
	Tool.GripForward = Vector3.new(-1,0,0)
	Tool.GripPos = Vector3.new(0,0,-.4)
	Tool.GripRight = Vector3.new(0,1,0)
	Tool.GripUp = Vector3.new(0,0,1)


	sound:Remove()
	enabled = true

end


function onEquipped()
	Calibrate.Volume = 1
end

function onUnequipped()
	Calibrate.Volume = 0
end


script.Parent.Activated:connect(onActivated)
script.Parent.Equipped:connect(onEquipped)
script.Parent.Unequipped:connect(onUnequipped)
