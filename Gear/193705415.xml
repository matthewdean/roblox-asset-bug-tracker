&#9;if Dropped then return end
&#9;
&#9;local root = Tool.Parent:FindFirstChild(&quot;HumanoidRootPart&quot;)
&#9;if root then
&#9;&#9;local model = game:GetService(&quot;InsertService&quot;):LoadAsset(193299081)
&#9;&#9;model.Parent = workspace
