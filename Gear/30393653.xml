		<Item class="LocalScript" referent="RBX13">
			<Properties>
				<bool name="Disabled">false</bool>
				<Content name="LinkedSource"><null></null></Content>
				<string name="Name">Local Gui</string>
				<ProtectedString name="Source">local Tool = script.Parent;
local anim = nil
enabled = true
local con = nil

function onButton1Down(mouse)
	if not enabled then
		return
	end

	enabled = false

	anim:Play()
	Tool.mode.Value = math.random(1,9)

	enabled = true

end


function onEquippedLocal(mouse)

	if mouse == nil then
		print(&quot;Mouse not found&quot;)
		return 
	end
	anim = Tool.Parent:FindFirstChild(&quot;Humanoid&quot;):LoadAnimation(Tool.saxtest)
	mouse.Icon = &quot;http://www.roblox.com/asset/?id=1088199&quot;
	con = mouse.Button1Down:connect(function() onButton1Down(mouse) end)
end

function onUnequippedLocal()

	if anim ~= nil then
		anim:Stop()
		anim:remove()
	end
	if con ~= nil then
		con:disconnect()
	end

end

Tool.Unequipped:connect(onUnequippedLocal)
Tool.Equipped:connect(onEquippedLocal)
</ProtectedString>
