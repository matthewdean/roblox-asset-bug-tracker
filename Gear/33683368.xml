				<string name="Name">LocalScript</string>
				<ProtectedString name="Source">local Tool = script.Parent
local blow = nil

local sound = Instance.new(&quot;Sound&quot;)
sound.Parent  = Tool.Handle
local sounds = {&quot;32727625&quot;,&quot;32727646&quot;,&quot;32727664&quot;,&quot;32727673&quot;}
sound.Volume = 0.01

for i = 1, #sounds do
	sound.SoundId = &quot;http://www.roblox.com/asset/?id=&quot; .. sounds[i]
	sound:Play()	
end

sound:Stop()
sound.Volume = 0.6

function onEquipped()
	sound:Stop()
	blow = Tool.Parent:FindFirstChild(&quot;Humanoid&quot;):LoadAnimation(Tool.hornblow)
end

function unEquipped()
	blow:remove()
	sound:Stop()
end

local enabled = false

function onActivated()

	if enabled then
		return
	end

	enabled = true

	blow:Play(0.1,1,0.9)
	Tool.GripForward = Vector3.new(1,0,0)
	Tool.GripPos = Vector3.new(0.3,0.2,-0.2)
	Tool.GripRight = Vector3.new(0,0,-1)
	sound.SoundId = &quot;http://www.roblox.com/asset/?id=&quot; .. sounds[math.random(1,#sounds)]
	sound.Pitch = math.random(0.7,1)
	sound:Play()

	wait(2.3)

	Tool.GripForward = Vector3.new(0,0,-1)
	Tool.GripPos = Vector3.new(-0.05, 0.12, 0.37)
	Tool.GripRight = Vector3.new(1, 0, 0)

	enabled = false

end

Tool.Activated:connect(onActivated)
Tool.Equipped:connect(onEquipped)
Tool.Unequipped:connect(onUnequipped)

