				<string name="Name">LaserBlast</string>
				<ProtectedString name="Source">ball = script.Parent
damage = 25

debris = game:GetService(&quot;Debris&quot;)
debris:AddItem(ball, 10)

HitSound = Instance.new(&quot;Sound&quot;)
HitSound.Name = &quot;HitSound&quot;
HitSound.SoundId = &quot;http://www.roblox.com/asset/?id=25918935&quot;
HitSound.Pitch = .5
HitSound.Parent = ball


function getPlayerHats(player)
	
	local hatTable = {}
	local stuff = player:GetChildren()
	for i = 1, #stuff do
		if stuff[i].className == &quot;Hat&quot; then
			table.insert(hatTable,stuff[i])
		end
	end
	return hatTable

end

function onTouched(hit)
	local humanoid = hit.Parent:findFirstChild(&quot;Humanoid&quot;)
	
	connection:disconnect()
	local bf = ball:FindFirstChild(&quot;BulletFloat&quot;)
	if (bf ~= nil) then bf:Remove() end

	if hit:FindFirstChild(&quot;GravityScript&quot;) then ball:remove() return end
	local script = ball.GravityScript:clone()
	if humanoid then
		script.Parent = humanoid.Parent:FindFirstChild(&quot;Torso&quot;)
	elseif hit.Anchored == false then
		hit:BreakJoints()
		script.Parent = hit
	end
	script.Disabled = false

	ball:remove()
end

function tagHumanoid(humanoid)
	-- todo: make tag expire
	local tag = ball:findFirstChild(&quot;creator&quot;)
	if tag ~= nil then
		local new_tag = tag:clone()
		new_tag.Parent = humanoid
		debris:AddItem(new_tag, 1)
	end
end



connection = ball.Touched:connect(onTouched)


