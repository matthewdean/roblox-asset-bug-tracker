				<string name="Name">WatchScript</string>
				<ProtectedString name="Source">local frameInterval = .35
local numFrames = 11
local reloadTime = numFrames * frameInterval + 1
local currentFrame = 0
local speedUp = 2.5

local frames = {}

local Tool = script.Parent;

local player
local torso
local hum

local running = false

function onActivated()

&#9;if not Tool.Enabled then return end
&#9;Tool.Enabled = false
&#9;running = true

&#9;Tool.Handle.OpenSound:play()
&#9;local curF = currentFrame
&#9;local i = (curF + 1) % numFrames
&#9;while i ~= curF and torso and hum and player do
&#9;&#9;torso.Anchored = true
&#9;&#9;local f = frames[i]
&#9;&#9;if f and torso and hum then
&#9;&#9;&#9;torso.CFrame = f[&quot;TorsoCFrame&quot;]
&#9;&#9;&#9;hum.Health = f[&quot;Health&quot;]
&#9;&#9;&#9;frames[i] = nil
&#9;&#9;&#9;Tool.Handle.OpenSound.Volume = 1
&#9;&#9;&#9;Tool.Handle.OpenSound:play()
&#9;&#9;&#9;wait(frameInterval / speedUp)
&#9;&#9;end
&#9;&#9;i = (i + 1) % numFrames
&#9;end
&#9;if torso then torso.Anchored = false torso.Velocity = Vector3.new(0,0,0) end

&#9;running = false


&#9;wait(reloadTime)

&#9;Tool.Enabled = true

end

function onEquipped()
&#9;Tool.Handle.OpenSound:play()
&#9;if player == Tool.Parent then 
&#9;&#9;for count = 0, 5 do
&#9;&#9;&#9;if count &lt;= 5 then
&#9;&#9;&#9;&#9;Tool.Handle.OpenSound.Volume = 1-(count /5)
&#9;&#9;&#9;&#9;Tool.Handle.OpenSound:play()
&#9;&#9;&#9;&#9;count = count+1
&#9;&#9;&#9;&#9;wait(frameInterval)
&#9;&#9;&#9;end
&#9;&#9;end
&#9;&#9;return 
&#9;end
&#9;frames = {}
&#9;player = Tool.Parent
&#9;torso = player:FindFirstChild(&quot;Torso&quot;)
&#9;hum = player:FindFirstChild(&quot;Humanoid&quot;)
&#9;local count = 0
&#9;while player and torso and torso.Parent and hum and Tool.Parent ~= game.Workspace do
&#9;&#9;if running then 
&#9;&#9;&#9;wait(frameInterval / speedUp)
&#9;&#9;else
&#9;&#9;&#9;if count &lt;= 5 then
&#9;&#9;&#9;&#9;Tool.Handle.OpenSound.Volume = count /5
&#9;&#9;&#9;&#9;Tool.Handle.OpenSound:play()
&#9;&#9;&#9;&#9;count = count+1
&#9;&#9;&#9;end
&#9;&#9;&#9;local f = {}
&#9;&#9;&#9;f[&quot;TorsoCFrame&quot;] = torso.CFrame
&#9;&#9;&#9;f[&quot;Health&quot;] = hum.Health
&#9;&#9;&#9;frames[currentFrame] = f
&#9;&#9;&#9;wait(frameInterval)
&#9;&#9;&#9;currentFrame = (currentFrame - 1) % numFrames
&#9;&#9;end
&#9;end
&#9;player = nil
&#9;print(&quot;unequipped&quot;)
end

function onUnequipped()
end

script.Parent.Activated:connect(onActivated)
script.Parent.Equipped:connect(onEquipped)
script.Parent.Unequipped:connect(onUnequipped)
