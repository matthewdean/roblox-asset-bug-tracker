&#9;end
end

function onEquippedLocal(mouse)
&#9;if mouse == nil then 
&#9;&#9;print(&quot;Mouse not found&quot;)
&#9;&#9;return
&#9;end
&#9;userMouse = mouse
&#9;checkConnection = Tool.Changed:connect( CheckEnabled )
end


function onUnequippedLocal()
&#9;if checkConnection then checkConnection:disconnect() end
end

